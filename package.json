  "scripts": {
    "test": "webpack --version && webpack --config=./webpack.v4.config.js --progress --colors",
    "test-production": "webpack --version && webpack --config=./webpack.v4.config.production.js -p --progress --colors",
    "build-dev": "babel webpack.src.config.babel.js > webpack.v4.config.js",
    "build-production": "babel webpack.src.prod.config.babel.js > webpack.v4.config.production.js",
    "standard": "standard",
    "test-builder": "webpack --version && webpack --config=./test/webpack.config.4x.babel.js --progress --colors",

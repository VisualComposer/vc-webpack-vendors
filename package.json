    "standard": "standard"
  },
  "resolutions": {
    "core-js": "3.2.1",
    "postcss": "7.0.32",
    "css-loader": "3.6.0",
    "color-name": "1.1.4",
    "eslint": "7.3.1",
    "less": "3.11.3"

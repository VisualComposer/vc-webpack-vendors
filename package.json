  "browserslist": [
    "ie >= 11",
    "last 2 version"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VisualComposer/vc-webpack-vendors.git"
  },
  "keywords": [
    "webpack",
    "vendor"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "public/dist/",
      "tests/",
      "**/public/js"
    ],
    "rules": {
      "jsx-quotes": [
        2,
        "prefer-single"
      ],
      "import/no-webpack-loader-syntax": "off"
    }
  },
